{
  "compilerOptions": {
    "outDir": "./tsc_output",
    "target": "es5",
    "module": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "./app/**/*"
  ],
  "exclude": [
    "./app/assets/*"
  ]
}